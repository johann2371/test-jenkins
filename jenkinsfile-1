pipeline {
    agent any

    options {
        buildDiscarder(logRotator(numToKeepStr: '5'))
    }

    stages {
        stage('Build & Sonar Scan') {
            steps {
                withSonarQubeEnv('sq1') {
                    bat 'mvnw.cmd clean verify sonar:sonar'
                }
            }
        }
    }
}
